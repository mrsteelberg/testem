    "sinon": "*",
    "async": "*",
    "chai": "*",
    "sinon-chai": "*",
    "bufferstream": "*",
    "sandboxed-module": "~0.1.3"
  },
  "bin": {
    "testem": "./testem.js"
  },
  "bundleDependencies": [],
  "optionalDependencies": {}
}
